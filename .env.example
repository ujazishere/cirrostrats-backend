# Environment Configuration
# Copy this file to .env and fill in your actual values

# Environment (dev/prod)
env=dev

# MongoDB Connection Strings
connection_string=mongodb+srv://username:password@cluster.mongodb.net/database_name
connection_string_uj=mongodb+srv://username:password@cluster.mongodb.net/database_name

# FlightAware API Key
ujazzzmay0525api=your_flightaware_api_key_here

# Telegram Bot Token
TELE_MAIN_BOT_TOKEN=your_telegram_bot_token_here

# Redis Configuration (if using Redis for caching)
REDIS_URL=redis://localhost:6379

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,https://your-domain.com
